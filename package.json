{
	"name": "gpt-commands.com",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"supabase:generate-types": "supabase gen types typescript --linked > src/lib/supabase-types.ts",
		"stripe:listen-local-payment": "stripe listen --forward-to localhost:5173/api/payment"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.21",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tailwindcss": "^3.2.7",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"dependencies": {
		"@neoconfetti/svelte": "^1.0.0",
		"@sendgrid/mail": "^7.7.0",
		"@supabase/auth-helpers-sveltekit": "^0.9.0",
		"@sveltejs/adapter-vercel": "^2.4.0",
		"@types/gtag.js": "^0.0.12",
		"classnames": "^2.3.2",
		"cross-env": "^7.0.3",
		"daisyui": "^2.51.3",
		"stripe": "^12.2.0",
		"supabase": "^1.45.2"
	},
	"type": "module"
}
